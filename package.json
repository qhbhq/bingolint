{
  "name": "bingolint",
  "version": "0.0.3",
  "description": "A collection of configuration files containing prettier, eslint, stylelint",
  "author": "qiuhongbingo@outlook.com",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsc --watch",
    "release": "tsc & release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qhbhq/bingolint"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "eslint",
    "prettier",
    "stylelint"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "release-it": "^14.4.1",
    "typescript": "^4.2.2"
  },
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "chore: release bingolint@${version}"
    }
  }
}
